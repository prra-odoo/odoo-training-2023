<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="res_user_offer_report">
        <t t-foreach="docs" t-as="users">
            <t t-call="web.html_container">
                <t t-call="web.external_layout">
                    <div class="page">
                        <h1>
                            <strong>Salesman: </strong>
                            <span t-field="users.name"/>
                        </h1> 
                        <div t-field="users.property_ids"/>
                    </div>
                </t>
            </t>
        </t>
    </template>
</odoo>