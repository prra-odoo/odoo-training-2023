<?xml version="1.0" encoding="utf-8"?>
<odoo>
            <!-- tree view -->
    <record id="property_offer_list_view" model="ir.ui.view">
        <field name="name">Property Offer List view</field>
        <field name="model">estate.property.offer</field>
        <field name="arch" type="xml">
            <tree string= "Property Offer">
                <field name="property_id" optional="show"/>
                <field name="price" optional="show"/>
                <field name="partner_id" optional="show"/>
                <field name="validity" optional="show"/>
                <field name="date_deadline" optional="show"/>
            </tree>
        </field>
    </record>        
</odoo>
