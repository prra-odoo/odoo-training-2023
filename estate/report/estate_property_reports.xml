<?xml version="1.0" encoding="UTF-8"?>
<odoo>
        <record id="report_estate_offer" model="ir.actions.report">
        <field name="name">Property Offers report</field>
        <field name="model">estate.property</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.report_estate_property_offers</field>
        <field name="report_file">estate.report_estate_property_offers</field>
        <!-- report file name is not necessary it is optional -->
        <field name="binding_model_id" ref="model_estate_property"/>
        </record>


        <record id="report_salesperson_property" model="ir.actions.report">
        <field name="name">Property</field>
        <field name="model">res.users</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.estate_property_offers_salesperson</field>
        <field name="report_file">estate.estate_property_offers_salesperson</field>
        <field name="binding_model_id" ref="model_res_users"/>
    </record>
</odoo>
