    <record id="estate_res_users_report" model="ir.actions.report">
        <field name="name">Report of res users</field>
        <field name="model">res.users</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.res_users_report_offers</field>
        <field name="report_file">estate.res_users_report_offers</field>
        <field name="binding_model_id" ref="base.model_res_users"/>
        <field name="binding_type">report</field>
    </record>

</odoo>