<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="report_property_offers" model="ir.actions.report">
        <field name="name">Property Offers Details</field>
        <field name="model">estate.property</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.report_template_property_offers</field>
        <field name="report_file">estate.report_template_property_offers</field>
        <field name="print_report_name">'Property - %s' % object.name</field>
        <field name="binding_model_id" ref="model_estate_property"/>
        <field name="binding_type">report</field>
    </record>

    <record id="report_users_property_offers" model="ir.actions.report">
        <field name="name">Users Property</field>
        <field name="model">res.users</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.report_template_users_property_offers</field>
        <field name="report_file">estate.report_template_users_property_offers</field>
        <field name="print_report_name">'Property - %s' % object.name</field>
        <field name="binding_model_id" ref="base.model_res_users"/>
        <field name="binding_type">report</field>
    </record>
</odoo>