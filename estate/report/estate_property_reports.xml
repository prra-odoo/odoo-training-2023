    <record id="estate_user_report" model="ir.actions.report">
        <field name="name">Estate Salesman Report</field>
        <field name="model">res.users</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.report_salesman_property</field>
        <field name="report_file">estate.report_salesman_property</field>
        <field name="binding_model_id" ref="base.model_res_users"/>
        <field name="binding_type">report</field>
    </record>
</odoo>
