<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="estate_property_reports" model="ir.actions.report">
        <field name="name">Report of property offers</field>
        <field name="model">estate.property</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.report_offers</field>
        <field name="report_file">estate.report_offers</field>
        <field name="binding_model_id" ref="model_estate_property"/>
