<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="report_estate_property" model="ir.actions.report">
        <field name="name">Estate Property</field>
        <field name="model">estate.property</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.report_property</field>
        <field name="report_file">estate.report_property</field>
        <field name="print_report_name">'Property - %s' % (object.name or 'Attendee').replace('/','')</field>
        <field name="binding_model_id" ref="estate.model_estate_property"/>
        <field name="binding_type">report</field>



    </record>


    <record id="res_users_property" model="ir.actions.report">
       <field name="name">User Properties</field>
       <field name="model">res.users</field>
       <field name="report_type">qweb-pdf</field>
       <field name="report_name">estate.report_users_property</field>
       <field name="report_file">estate.report_users_property</field>
       <field name="print_report_name">' %s' %(object.name or 'Attendee').replace('/','')</field>
       <field name="binding_model_id" ref="base.model_res_users"/>
      
    </record>
</odoo>
