<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <data>
        <!-- tags -->
        <record id="tag_1" model="estate.property.tags">
            <field name="name">cozy</field>
        </record>
        <record id="tag_2" model="estate.property.tags">
            <field name="name">garden</field>
        </record>
        <record id="tag_3" model="estate.property.tags">
            <field name="name">parking space</field>
        </record>
        <!-- types -->
        <record id="type_1" model="estate.property.type">
            <field name="name">Apartment</field>
        </record>
        <record id="type_2" model="estate.property.type">
            <field name="name">Bungalow</field>
        </record>
        <record id="type_3" model="estate.property.type">
            <field name="name">Vilaa</field>
        </record>
        <!-- properties -->
        <record id="property_1" model="estate.property">
            <field name="name">Shreeji Apartments</field>
            <field name="expected_price">10000</field>
            <field name="tag_ids" eval="[(6,0,[ref('tag_1'),ref('tag_3')])]"/>
            <field name="property_type_id">1</field>
        </record>
        <record id="property_2" model="estate.property">
            <field name="name">Godrej</field>
            <field name="expected_price">200000</field>
            <field name="tag_ids" eval="[(6,0,[ref('tag_2'),ref('tag_3')])]"/>
            <field name="property_type_id">2</field>
        </record>
        <record id="property_3" model="estate.property">
            <field name="name">Alux</field>
            <field name="expected_price">10000</field>
            <field name="tag_ids" eval="[(6,0,[ref('tag_2'),ref('tag_1')])]"/>
            <field name="property_type_id">3</field>
        </record>
        <!-- offers -->
        <record id="offer_1" model="estate.property.offer">
            <field name="price">10000</field>
            <field name="property_id">1</field>
        </record>
        <record id="offer_2" model="estate.property.offer">
            <field name="price">200000</field>
            <field name="property_id">2</field>
        </record>
        <record id="offer_3" model="estate.property.offer">
            <field name="price">15000</field>
            <field name="property_id">3</field>
        </record>
        <record id="offer_4" model="estate.property.offer">
            <field name="price">12000</field>
            <field name="property_id">1</field>
        </record>
    </data>
</odoo>