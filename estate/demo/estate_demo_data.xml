<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<!-- Property Type Demo Data -->
		<record id="demo_property_type_1" model="estate.property.type">
			<field name="name">Apartment</field>
		</record>
		<record id="demo_property_type_2" model="estate.property.type">
			<field name="name">Bungalow</field>
		</record>
		<record id="demo_property_type_3" model="estate.property.type">
			<field name="name">Villa</field>
		</record>
		<!-- Property Tag Demo Data -->
		<record id="demo_property_tag_1" model="estate.property.tag">
			<field name="name">cozy</field>
			<field name="colors">2</field>
		</record>
		<record id="demo_property_tag_2" model="estate.property.tag">
			<field name="name">renovated</field>
			<field name="colors">4</field>
		</record>
		<!-- Property Demo Data -->
		<record id="demo_property_1" model="estate.property">
			<field name="name">Swagat</field>
			<field name="state">new</field>
			<field name="property_type_id" ref="demo_property_type_3"/>
			<!-- <field name="tag_ids" eval="[(6, 0, [ref('estate.property.tag.demo_property_tag_1'), ref('estate.property.tag.demo_property_tag_2')])]"/> -->
			<field name="description">A nice and big villa</field>
			<field name="postcode">12345</field>
			<field name="date_availability">2020-02-02</field>
			<field name="expected_price">3600000</field>
			<field name="bedrooms">6</field>
			<field name="living_area">100</field>
			<field name="facades">4</field>
			<field name="garage">True</field>
			<field name="garden">True</field>
			<field name="garden_area">100000</field>
			<field name="garden_orientation">south</field>
		</record>
		<record id="demo_property_2" model="estate.property">
			<field name="name">Ghanshyam</field>
			<field name="state">canceled</field>
			<field name="property_type_id" ref="demo_property_type_1"/>
			<!-- <field name="tag_ids" eval="[(6, 0, [ref('estate.property.tag.demo_property_tag_1')])]"/> -->
			<field name="description">Home in gandhinagar</field>
			<field name="postcode">54321</field>
			<field name="date_availability">1970-01-01</field>
			<field name="expected_price">3200000</field>
			<field name="selling_price">3200000</field>
			<field name="bedrooms">1</field>
			<field name="living_area">10</field>
			<field name="facades">4</field>
			<field name="garage">False</field>
		</record>
	</data>
</odoo>