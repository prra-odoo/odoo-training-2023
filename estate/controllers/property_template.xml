<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="index">
         <t t-call="website.layout">
            <t t-set="title">Properties</t>
            <div class="oe_structure">
                <div class="container">
                     <t t-foreach="properties" t-as="property">
                         <p><t t-esc="property.id"/> <t t-esc="property.name"/></p>
                     </t>
                 </div>
            </div>
        </t>
    </template>
</odoo>