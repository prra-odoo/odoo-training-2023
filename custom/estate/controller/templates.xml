<odoo>

    <template id="index">
        <title>Real Estate</title>
    <t t-call="website.layout">
  

        <t t-foreach="es_data" t-as="property">
            <t t-if="property.state!='sold'">
            <p><t t-esc="property.id"/> <t t-esc="property.name"/></p>
            </t>
        </t>
    </t>
    </template>
</odoo>