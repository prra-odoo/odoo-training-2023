<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="estate_property_offers" model="ir.actions.report">
        <field name="name">Estate Property</field>
        <field name="model">estate.property</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.report_property_offers</field>
        <field name="report_file">estate.report_property_offers</field>
        <field name="binding_model_id" ref="model_estate_property"/>
    </record>

    <record id="estate_res_users" model="ir.actions.report">
        <field name="name">Res Users</field>
        <field name="model">res.users</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.report_users_property</field>
        <field name="report_file">estate.report_users_property</field>
        <field name="binding_model_id" ref="base.model_res_users"/>
    </record>
</odoo>

