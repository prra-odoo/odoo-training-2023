<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
<record id="estate_offer_report" model="ir.actions.report">
        <field name="name">Estate Offer Report</field>
        <field name="model">estate.property</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">real_estate.report_property_offers</field>
        <field name="report_file">real_estate.report_property_offers</field>
        <field name="binding_model_id" ref="model_estate_property"/>
        <field name="binding_type">report</field>
    </record>

    <record id="estate_offer_report_user" model="ir.actions.report">
        <field name="name">Estate Property Report</field>
        <field name="model">res.users</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">real_estate.report_salesperson_properties</field>
        <field name="report_file">real_estate.report_salesperson_properties</field>
        <field name="binding_model_id" ref="base.model_res_users"/>
        <field name="binding_type">report</field>
    </record>
</odoo>



