<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="estate_property_report_action" model="ir.actions.report">
        <field name="name">Property Offers</field>
        <field name="model">res.users</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">real_estate.report_property_offers</field>
        <field name="report_file">real_estate.report_property_offers</field>
        <field name="print_report_name">'Propert Offers - %s' % (object.name or 'Attendee').replace('/','')</field>
        <field name="binding_model_id" ref="model_estate_property"/>
        <field name="binding_type">report</field>
    </record>

    <record id="estate_report_resuser_action" model="ir.actions.report">
        <field name="name">Res Users</field>
        <field name="model">res.users</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">real_estate.report_property_resusers</field>
        <field name="report_file">real_estate.report_property_resusers</field>
        <field name="print_report_name">'Propert Offers - %s' % (object.name or 'Attendee').replace('/','')</field>
        <field name="binding_model_id" ref="base.model_res_users"/>
        <field name="binding_type">report</field>
    </record>
</odoo>