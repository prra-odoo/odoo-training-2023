<?xml version="1.0" encoding="utf-8"?>
    <odoo>
        <record id="real_estate_property_type_view" model="ir.actions.act_window">
            <field name="name">Property Types</field>
            <field name="res_model">real.estate.property.type</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Define a new property type
            </p>
            <p>
                i.e. House,Office,etc.
            </p>
            </field>
        </record>
        <record id="real_estate_proeprty_type_tree" model="ir.ui.view">
            <field name="name">real.estate.property.type.tree</field>
            <field name="model">real.estate.property.type</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="sequence" widget="handle"/>
                    <field name="name"/>
                </tree>
            </field>
        </record>
        <record id="real_estate_property_type_form" model="ir.ui.view">
            <field name="name">real.estate.property.type.form</field>
            <field name="model">real.estate.property.type</field>
            <field name="arch" type="xml">
                <form name="Property Type">
                    <sheet>
                        <div name="button_box" class="oe_button_box">
                            <button class="oe_stat_button" type="action" name="%(real_estate_property_offer_view)d" icon="fa-money">
                                <field name="offer_count"/>
                            </button>
                        </div>
                        <h1>
                            <field name="name"/>
                        </h1>
                        <notebook>
                            <page name="Properties">
                                <field name="property_ids">
                                    <tree>
                                    <field name="name" string="Title"/>
                                    <field name="expected_price"/>
                                    <field name="state" string="Status"/>
                                    </tree>
                                </field>
                            </page>
                        </notebook>
                       
                    </sheet>
                </form>
            </field>
        </record>
    </odoo>