<?xml version="1.0"?>
 <odoo>
    <record id="real_estate_tag_form" model="ir.ui.view">
            <field name="name">real.estare.view.form</field>
            <field name="model">estate.property.tag</field>
            <field name="arch" type="xml">
            <form string="type">
                    <sheet>
                        <h1>
                            <field name="name"/>
                            <!-- <field name="type_id"/> -->
                        </h1>
                    </sheet>
            </form>
            </field>
    </record>
     <record id="estate_tag_view_tree" model="ir.ui.view">
        <field name="name">real.estate.view.tree</field>
        <field name="model">estate.property.tag</field>
        <field name="arch" type="xml">
            <tree  editable="bottom">
                <field name="name"/>
                
            </tree>
        </field>
    </record>
 </odoo>                