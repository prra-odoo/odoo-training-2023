<odoo>
    <record id="estate_res_users_form_view" model="ir.ui.view">
        <field name="name">Real Esate Res User</field>
        <field name="model">res.users</field>
        <!-- <field name="mode">primary</field> -->
        <field name="inherit_id" ref="base.view_users_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='access_rights']" position="after">
                <page string="real estate " name="estate">
                    <field name="property_ids" domain="['|',('state', '=' , 'new'),('state','=','offer_recieved')]"/>
                </page>
            </xpath>
        </field>
    </record>
</odoo>